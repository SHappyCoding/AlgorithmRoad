public class RBT_Node {
    public Integer key;
    public RBT_Node left, right,parent;
    public Colors color;


    public RBT_Node(Integer key)
    {
        this.key = key;
        color =Colors.RED;//默认构建红色节点
        left = right = parent = null;
    }
    public RBT_Node(Integer key,Colors color)
    {
        this.key = key;
        this.color =color;
        left = right = parent = null;
    }
}
